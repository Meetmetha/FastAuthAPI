{
  "name": "nestjs",
  "version": "1.0.0",
  "description": "Base Project",
  "author": "Mitesh Metha",
  "private": false,
  "license": "MIT",
  "keywords": [
    "nestjs-boilerplate",
    "nestjs-objection",
    "nestjs-starter-template",
    "nestjs-template"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main.js",
    "start:dist": "node dist/src/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "knex": "knex --knexfile knexfile.ts",
    "migrate:up": "npm run knex migrate:latest",
    "migrate:down": "npm run knex migrate:rollback",
    "migrate:make": "npm run knex migrate:make -- -x ts",
    "migrate:list": "npm run knex migrate:list",
    "seed:run": "npm run knex seed:run",
    "seed:make": "npm run knex seed:make -- -x ts"
  },
  "dependencies": {
    "@nestjs/common": "^7.5.5",
    "@nestjs/config": "^0.6.0",
    "@nestjs/core": "^7.5.5",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/mongoose": "^7.1.0",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^7.5.5",
    "@squareboat/nest-events": "0.0.5",
    "bcrypt": "*",
    "chalk": "^4.1.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "cli-table3": "^0.6.0",
    "dotenv": "^8.2.0",
    "express-rate-limit": "^5.2.3",
    "helmet": "^4.2.0",
    "inquirer": "^7.3.3",
    "knex": "^0.21.12",
    "moment": "^2.29.1",
    "mongoose": "^5.11.12",
    "mysql2": "^2.2.5",
    "objection": "^2.2.3",
    "passport": "^0.4.1",
    "query-string": "^6.13.7",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.3",
    "serve-favicon": "^2.5.0",
    "uuid": "^8.3.1",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.5.3",
    "@nestjs/schematics": "^7.2.2",
    "@nestjs/testing": "^7.5.5",
    "@types/chalk": "^2.2.0",
    "@types/express": "^4.17.9",
    "@types/jest": "^26.0.16",
    "@types/node": "^14.14.10",
    "@types/supertest": "^2.0.8",
    "@types/yargs": "^15.0.11",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-import": "^2.22.1",
    "prettier": "^2.2.1",
    "supertest": "^6.0.1",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.11",
    "ts-node": "^9.1.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "@libs/core/(.*)": "<rootDir>/libs/core/src/$1",
      "@libs/core": "<rootDir>/libs/core/src"
    }
  }
}
